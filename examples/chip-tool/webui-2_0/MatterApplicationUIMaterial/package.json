  "build-image": "docker build -t nginx-docker-image .",
  "remove-image": "docker rmi nginx-docker-image",
  "run-container": "docker run --name nginx-docker-container -d -p 8080:80 nginx-docker-image",
  "remove-container": "docker rm -f nginx-docker-container",
  "containerize": "npm run build-image && npm run run-container",
  "recontainerize": "npm run remove-container && npm run build-image && npm run run-container"
